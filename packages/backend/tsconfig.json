{
  "compilerOptions": {
    "moduleResolution": "Node",
    "target": "ES2022",
    "allowJs": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": true,
    "strictNullChecks": true,
    "downlevelIteration": true,
    "sourceMap": true,
    "jsx": "react",
    "baseUrl": ".",
    "outDir": "dist"
  }
}
